# StudyWS Backend Environment Configuration

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=studyws
DB_USER=studyws_user
DB_PASSWORD=your_secure_password_here

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Token expiry times (in seconds)
JWT_ACCESS_EXPIRY=3600 # 1 hour
JWT_REFRESH_EXPIRY=604800 # 7 days

# Application
APP_ENV=development
APP_DEBUG=true
SERVER_PORT=8080
